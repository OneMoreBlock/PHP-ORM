{
    "name": "takeright/php-orm",
    "type": "library",
    "description": "A simple and inituative API for DB",
    "keywords": [
        "takeright",
        "php-orm"
    ],
    "homepage": "https://github.com/takeright/php-orm",
    "license": "MIT",
    "authors": [
        {
            "name": "Ram Bhosale",
            "email": "rambhosale9@gmail.com",
            "homepage": "https://github.com/takeright",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "takeright\\php-orm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "takeright\\php-orm\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
